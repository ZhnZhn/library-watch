{
  "name": "library-watch",
  "version": "0.12.0",
  "description": "Web app Library Watch gives the ability to view information about GitHub's repositories, NPM's packages, StackOverflows's questions.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "lint": "eslint -c .eslintrc.js ./src > lint.txt",
    "lint:build": "eslint -c .eslintrc-build.js ./src > lint-build.txt",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore ./**/__tests__",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "css:min": "gulp",
    "build:dev:dll": "webpack --color --progress --config webpack.dev.dll.js",
    "build:dev:dll-jsx": "webpack --color --progress --config webpack.dev.dll-jsx.js",
    "build:dll": "webpack --color --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --color --progress --config webpack.dll-jsx.js",
    "build:prod": "webpack --color --progress --config webpack.prod.js",
    "stats": "npm run stats:json && npm run stats:report",
    "stats:json": "webpack --config webpack.test.js --profile --json > stats.json",
    "stats:report": "webpack-bundle-analyzer stats.json --no-open --mode static",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Yevhenii Molodyi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/library-watch.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.2",
    "babel-loader": "^8.2.3",
    "del": "^6.0.0",
    "eslint": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-rev": "^9.0.0",
    "html-webpack-plugin": "^5.5.0",
    "react-refresh": "^0.10.0",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^3.11.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "chart.js": "^3.7.0",
    "interactjs": "^1.3.4",
    "localforage": "^1.5.7",
    "papaparse": "^5.3.1",
    "raven-js": "^3.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflux-core": "^1.0.0",
    "timeago.js": "^4.0.2"
  }
}
