{
  "name": "library-watch",
  "version": "0.12.0",
  "description": "Library Watch is a RESTFul client. With it you can view information about libraries, repositories from GitHub, NPM api services.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc-build.json ./src > lint.txt",
    "test": "ava --verbose",
    "test:watch": "npm run test -- --watch",
    "test:one": "ava --config ava-one.config.js --verbose --watch --source='menubadge*'",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "build": "webpack --colors --progress",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js",
    "build:css": "gulp"
  },
  "author": "Yevhenii Molodyi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/library-watch.git"
  },
  "jspm": {
    "dependencies": {
      "babel-runtime": "npm:babel-runtime@^6.26.0",
      "browser-filesaver": "npm:browser-filesaver@^1.1.1",
      "chart.js": "npm:chart.js@^2.8.0",
      "classnames": "npm:classnames@^2.2.6",
      "create-react-class": "npm:create-react-class@^15.6.3",
      "interactjs": "npm:interactjs@^1.3.4",
      "localforage": "npm:localforage@1.5.7",
      "lodash.merge": "npm:lodash.merge@^4.6.2",
      "papaparse": "npm:papaparse@^5.0.2",
      "prop-types": "npm:prop-types@^15.7.2",
      "raven-js": "npm:raven-js@3.27.0",
      "react": "npm:react@^16.9.0",
      "react-dom": "npm:react-dom@^16.9.0",
      "reflux": "npm:reflux@5.0.4",
      "timeago.js": "npm:timeago.js@2.0.5"
    },
    "devDependencies": {}
  },
  "devDependencies": {
    "ava": "file:node_modules_b7/node_modules/ava",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.23",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "del": "^5.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-processing-plugin": "^0.1.2",
    "jsdom": "^15.2.1",
    "jspm": "^0.16.55",
    "sinon": "^1.17.5",
    "webpack": "^3.12.0"
  },
  "dependencies": {
