{
  "name": "library-watch",
  "version": "0.11.0",
  "description": "Library Watch is a RESTFul client. With it you can view information about libraries, repositories from GitHub, NPM api services.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src/*.js*",
    "test": "ava src/**/__tests__/**/*.test.js --verbose",
    "test:watch": "npm run test -- --watch",
    "test:one": "ava src/**/__tests__/**/*.test.js --verbose --watch --source='menubadge*'",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "lint": "eslint -c .eslintrc-build.json ./src > lint.txt",
    "build": "webpack --colors --progress",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js",
    "gulp": "gulp",
    "start:emitter": "chokidar-socket-emitter",
    "start:web": "http-server -c-1 -p8080"
  },
  "author": "ZhnZhn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/library-watch.git"
  },
  "jspm": {
    "dependencies": {
      "babel-runtime": "npm:babel-runtime@^6.23.0",
      "browser-filesaver": "npm:browser-filesaver@^1.1.1",
      "chart.js": "npm:chart.js@^2.7.1",
      "classnames": "npm:classnames@^2.2.5",
      "create-react-class": "npm:create-react-class@^15.6.3",
      "interactjs": "npm:interactjs@^1.3.4",
      "localforage": "npm:localforage@1.5.7",
      "lodash.merge": "npm:lodash.merge@^4.6.0",
      "prop-types": "npm:prop-types@^15.6.2",
      "raven-js": "npm:raven-js@3.27.0",
      "react": "npm:react@^16.7.0",
      "react-dom": "npm:react-dom@^16.7.0",
      "reflux": "npm:reflux@5.0.4",
      "timeago.js": "npm:timeago.js@2.0.5"
    },
    "devDependencies": {
      "systemjs-hot-reloader": "npm:systemjs-hot-reloader@^1.1.0"
    }
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.23",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chokidar-socket-emitter": "^0.6.0",
    "del": "^2.2.2",
    "enzyme": "^3.8.0",
    "eslint": "^5.12.1",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.12",
    "gulp-concat": "^2.6.0",
    "gulp-inject": "^4.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-processing-plugin": "^0.1.2",
    "jspm": "^0.16.53",
    "react-addons-test-utils": "^15.6.2",
    "sinon": "^1.17.5",
    "webpack": "^3.12.0"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-2",
        "react"
      ]
    },
    "files": [
      "src/**/__tests__/**/*.test.js"
    ]
  },
  "dependencies": {
    "browser-filesaver": "^1.1.1",
    "chart.js": "^2.7.3",
    "classnames": "^2.2.6",
    "create-react-class": "^15.6.3",
    "interactjs": "^1.3.4",
    "localforage": "^1.5.7",
    "lodash.merge": "^4.6.1",
    "raven-js": "^3.27.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "reflux": "^5.0.4",
    "timeago.js": "^2.0.5"
  }
}
