{
  "name": "library-watch",
  "version": "0.12.0",
  "description": "Web app Library Watch gives the ability to view information about GitHub's repositories, NPM's packages, StackOverflows's questions.",
  "main": "index.js",
  "scripts": {
    "lint:build": "eslint -c .eslintrc-build.js ./src > lint-build.txt",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore ./**/__tests__",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "css:min": "gulp",
    "build:dev:dll": "webpack --colors --progress --config webpack.dev.dll.js",
    "build:dev:dll-jsx": "webpack --colors --progress --config webpack.dev.dll-jsx.js",
    "build:dll": "webpack --colors --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --colors --progress --config webpack.dll-jsx.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Yevhenii Molodyi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/library-watch.git"
  },
  "jspm": {
    "dependencies": {
      "@babel/runtime": "npm:@babel/runtime@^7.10.2",
      "browser-filesaver": "npm:browser-filesaver@^1.1.1",
      "chart.js": "npm:chart.js@^2.9.3",
      "clsx": "npm:clsx@^1.1.1",
      "interactjs": "npm:interactjs@^1.3.4",
      "localforage": "npm:localforage@1.5.7",
      "lodash.merge": "npm:lodash.merge@^4.6.2",
      "papaparse": "npm:papaparse@^5.2.0",
      "prop-types": "npm:prop-types@^15.7.2",
      "raven-js": "npm:raven-js@^3.27.2",
      "react": "npm:react@^16.13.1",
      "react-dom": "npm:react-dom@^16.13.1",
      "reflux-core": "npm:reflux-core@^1.0.0",
      "timeago.js": "npm:timeago.js@2.0.5"
    },
    "devDependencies": {}
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jspm": "^0.16.55",
    "terser-webpack-plugin": "^3.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "browser-filesaver": "^1.1.1",
    "chart.js": "^2.9.3",
    "clsx": "^1.1.1",
    "interactjs": "^1.3.4",
    "localforage": "^1.5.7",
    "lodash.merge": "^4.6.2",
    "papaparse": "^5.2.0",
    "raven-js": "^3.27.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reflux-core": "^1.0.0",
    "timeago.js": "^2.0.5"
  }
}
