{
  "name": "library-watch",
  "version": "0.10.0",
  "description": "Library Watch is a RESTFul client. With it you can view information about libraries, repositories from GitHub, NPM api services.",
  "main": "index.js",
  "scripts": {
    "test": "ava test/**/*.test.js --verbose",
    "test:watch": "npm run test -- --watch",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "build": "webpack --colors --progress",
    "gulp": "gulp",
    "start": "http-server -p8080"
  },
  "author": "ZhnZhn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/library-watch.git"
  },
  "jspm": {
    "dependencies": {
      "browser-filesaver": "npm:browser-filesaver@^1.1.1",
      "chart.js": "npm:chart.js@^2.2.2",
      "classnames": "npm:classnames@^2.2.5",
      "interact.js": "npm:interact.js@^1.2.6",
      "jszip": "npm:jszip@^3.1.3",
      "localforage": "npm:localforage@^1.4.2",
      "raven-js": "npm:raven-js@^3.7.0",
      "react": "npm:react@^15.3.2",
      "react-dom": "npm:react-dom@^15.3.2",
      "reflux": "npm:reflux@^0.4.1",
      "timeago.js": "npm:timeago.js@^1.1.0"
    },
    "devDependencies": {}
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-recharts": "^1.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "enzyme": "^2.4.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.12",
    "gulp-concat": "^2.6.0",
    "html-webpack-plugin": "^2.21.0",
    "jspm": "^0.16.45",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^1.17.5",
    "webpack": "^1.13.1"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-2",
        "react"
      ]
    },
    "source": [
      "!src/**/*",
      "!js/**/*",
      "!jspm_packages/**/*",
      "!lib/**/*",
      "!local_modules/**/*",
      "!node_modules/**/*",
      "test/**/*"
    ]
  }
}
