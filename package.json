{
  "name": "library-watch",
  "version": "0.11.0",
  "description": "Library Watch is a RESTFul client. With it you can view information about libraries, repositories from GitHub, NPM api services.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src/*.js*",
    "test": "ava src/**/__tests__/**/*.test.js --verbose",
    "test:watch": "npm run test -- --watch",
    "test:one": "ava src/**/__tests__/**/*.test.js --verbose --watch --source='menubadge*'",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore test.js",
    "bundle": "jspm bundle js/**/*  - [js/**/*] dependency-bundle.js",
    "build": "webpack --colors --progress",
    "gulp": "gulp",
    "start:emmiter": "chokidar-socket-emitter",
    "start:web": "http-server -c-1 -p7070"
  },
  "author": "ZhnZhn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/library-watch.git"
  },
  "jspm": {
    "dependencies": {
      "browser-filesaver": "npm:browser-filesaver@^1.1.1",
      "chart.js": "npm:chart.js@^2.4.0",
      "classnames": "npm:classnames@^2.2.5",
      "interact.js": "npm:interact.js@^1.2.6",
      "localforage": "npm:localforage@^1.4.3",
      "lodash.merge": "npm:lodash.merge@^4.6.0",
      "raven-js": "npm:raven-js@^3.12.0",
      "react": "npm:react@^15.4.2",
      "react-dom": "npm:react-dom@^15.4.2",
      "reflux": "npm:reflux@^5.0.3",
      "timeago.js": "npm:timeago.js@^2.0.4"
    },
    "devDependencies": {
      "systemjs-hot-reloader": "npm:systemjs-hot-reloader@^1.1.0"
    }
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-recharts": "^1.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "chokidar-socket-emitter": "^0.6.0",
    "del": "^2.2.2",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^2.0.12",
    "gulp-concat": "^2.6.0",
    "gulp-inject": "^4.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rev": "^7.1.2",
    "html-webpack-plugin": "^2.21.0",
    "jspm": "^0.16.45",
    "react-addons-test-utils": "^15.4.2",
    "sinon": "^1.17.5",
    "webpack": "^1.14.0"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-2",
        "react"
      ]
    },
    "files": [
      "src/**/__tests__/**/*.test.js"
    ]
  },
  "dependencies": {
    "raven-js": "^3.9.2"
  }
}
