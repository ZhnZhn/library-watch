{
  "name": "library-watch",
  "version": "0.12.0",
  "description": "Web app Library Watch gives the ability to view information about GitHub's repositories, NPM's packages, StackOverflows's questions.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js",
    "lint": "eslint -c .eslintrc.js ./src > lint.txt",
    "lint:build": "eslint -c .eslintrc-build.js ./src > lint-build.txt",
    "transpile": "babel ./src --out-dir ./js --source-maps --ignore ./**/__tests__",
    "transpile:all": "babel ./src --out-dir ./js --source-maps",
    "bundle": "jspm bundle js/**/* - [js/**/*] dependency-bundle.js",
    "css:min": "gulp",
    "build:dev:dll": "webpack --color --progress --config webpack.dev.dll.js",
    "build:dev:dll-jsx": "webpack --color --progress --config webpack.dev.dll-jsx.js",
    "build:dll": "webpack --color --progress --config webpack.dll.js",
    "build:dll-jsx": "webpack --color --progress --config webpack.dll-jsx.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Yevhenii Molodyi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZhnZhn/library-watch.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "del": "^6.0.0",
    "eslint": "^7.12.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-inject": "^5.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "html-webpack-plugin": "^5.3.1",
    "react-refresh": "^0.9.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "browser-filesaver": "^1.1.1",
    "chart.js": "^2.9.4",
    "clsx": "^1.1.1",
    "interactjs": "^1.3.4",
    "localforage": "^1.5.7",
    "lodash.merge": "^4.6.2",
    "papaparse": "^5.2.0",
    "raven-js": "^3.27.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reflux-core": "^1.0.0",
    "timeago.js": "^2.0.5"
  }
}
